{
  "presets": [
    [
      "babel-preset-es2015",
      {
        "modules": false
      }
    ],
    "babel-preset-stage-3"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "~": "/",
        "@common/*": "common/*",
        "@components/*": "components/*",
        "@store/*": "store/*",
        "@router/*": "router/*"
      }
    }]
  ]
}
